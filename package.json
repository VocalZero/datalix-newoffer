{
  "name": "datalix-newoffer",
  "version": "1.0.0",
  "main": "./build/app.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "tsc --skipLibCheck",
    "start": "tsc --skipLibCheck && node ./build/app.js",
    "start:prod": "node ./build/app.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Kacper Mura",
  "license": "ISC",
  "description": "This tool sends a notification via a Discord webhook when a new sale dedicated server is available from the hosting provider Datalix.",
  "dependencies": {
    "axios": "^1.13.2",
    "crypto": "^1.0.1",
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^17.2.3",
    "log4js": "^6.9.1",
    "node-cron": "^4.2.1",
    "node-html-parser": "^7.0.1"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}

